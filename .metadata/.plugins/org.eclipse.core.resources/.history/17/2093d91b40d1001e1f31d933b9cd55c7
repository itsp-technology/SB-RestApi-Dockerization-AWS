package com.itsp.repo;

import java.util.HashMap;
import java.util.Map;

import org.springframework.stereotype.Repository;

import com.itsp.model.EmployeeModel;

@Repository
public class EmployeeRepo {
	
	Map<Long,Object> mapRepo = new HashMap<Long, Object>();
	
	public String employeeRepo(EmployeeModel model) {
		Object obj = mapRepo.put(model.getId(), model);
		if(obj!=null) {
			return "success";
		}else {
			return "false";
		}
	}
	
	public Map<Long,Object> getAllEmployee(){
		return mapRepo;
	}

}
